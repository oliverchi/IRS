<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.net.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="640.0" prefWidth="800.0" styleClass="mainFxmlClass" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="irs.UserRegisterPage">
    <stylesheets>
        <URL value="@/irs/css/userregisterpage.css" />
    </stylesheets>
   <children>
      <Button fx:id="registerBtn" layoutX="458.0" layoutY="360.0" mnemonicParsing="false" onAction="#registerOnAction" text="Register" />
      <Label layoutX="25.0" layoutY="14.0" text="User Register Form">
         <font>
            <Font name="System Bold Italic" size="18.0" />
         </font></Label>
      <Label alignment="CENTER_RIGHT" layoutX="100.0" layoutY="150.0" prefWidth="120.0" text="User Name" />
      <Label layoutX="46.0" layoutY="41.0" text="Please make sure you have the correct information before registering an account" textFill="#e51010bd">
         <font>
            <Font name="Arial Narrow" size="14.0" />
         </font>
      </Label>
      <Label alignment="CENTER_RIGHT" layoutX="100.0" layoutY="200.0" prefWidth="120.0" text="Password" />
      <Label alignment="CENTER_RIGHT" layoutX="100.0" layoutY="250.0" prefWidth="120.0" text="Staff ID" />
      <Label alignment="CENTER_RIGHT" layoutX="100.0" layoutY="300.0" prefWidth="120.0" text="Clinic Position" />
      <TextField fx:id="nameField" alignment="CENTER" layoutX="230.0" layoutY="143.0" prefHeight="30.0" prefWidth="420.0" promptText="at least 6 characters" />
      <TextField fx:id="keyField" alignment="CENTER" layoutX="230.0" layoutY="193.0" prefHeight="30.0" prefWidth="420.0" promptText="at least 8 characters with 1 letter and 1 number" />
      <TextField fx:id="idField" alignment="CENTER" layoutX="230.0" layoutY="243.0" prefHeight="30.0" prefWidth="420.0" promptText="first two letters and following four numbers " />
      <TextField fx:id="positionField" alignment="CENTER" layoutX="230.0" layoutY="293.0" prefHeight="30.0" prefWidth="420.0" promptText="two letters such as RN, CN, NM, ND and DR" />
      <Label fx:id="errorMsg" layoutX="238.0" layoutY="336.0" textFill="#e51010bd">
         <font>
            <Font name="Arial Narrow" size="14.0" />
         </font>
      </Label>
      <Button fx:id="exitBtn" layoutX="561.0" layoutY="360.0" mnemonicParsing="false" onAction="#exitBtnOnAction" text="   EXIT   " />
   </children>
</AnchorPane>
